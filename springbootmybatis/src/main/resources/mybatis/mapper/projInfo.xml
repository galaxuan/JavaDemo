<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.demo.springbootmybatis.mapper.ProjInfoMapper">
    <select id="findProjInfoByName" parameterType="String" resultType="com.demo.springbootmybatis.entity.ProjInfo">
        select ProjName from proj_info where ProjName = #{value} LIMIT 10;
    </select>

    <select id="findProjInfoByNewCode" parameterType="long" resultType="com.demo.springbootmybatis.entity.ProjInfo">
        select ProjName from proj_info where NewCode = #{value} LIMIT 10;
    </select>
</mapper>